{
  "rewrites": [
    {
      "source": "/terminix",
      "destination": "/terminix.html"
    },
    {
      "source": "/united-landing",
      "destination": "/united-landing/index.html"
    },
    {
      "source": "/delta-landing",
      "destination": "/delta-landing/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 